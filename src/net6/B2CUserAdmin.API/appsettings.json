{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Hosting.Lifetime": "Debug"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "TenantId": "{the B2C tenant GUID}",
    "Authority": "https://login.microsoftonline.com/{B2C tenant name}.onmicrosoft.com/v2.0",
    "AuthorizationUrl": "https://login.microsoftonline.com/{B2C tenant name}.onmicrosoft.com/oauth2/v2.0/authorize",
    "ClientId": "{client id of the API app}",
    "ApplicationIdUri": "{application id uri of the API app. e.g. https://b2ctenant.onmicrosoft.com/{guid}}",
    "ClientSecret": "{client secret in the API app}",
    "TenantName": "{B2C tenant name}.onmicrosoft.com",
    "GraphApiBaseUrl": "https://graph.microsoft.com/beta",
    "Instance": "https://login.microsoftonline.com/"
  },
  "ApplicationInsights": {
    "ConnectionString": "{copy from the Azure portal}"
  },
  "Cors:Origins": [
    "https://localhost:5001",
    "http://localhost:55006",
    "http://localhost:51680"
  ]
}