{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Hosting.Lifetime": "Debug"
    }
  },
  "AzureAdB2C": {
    "Authority": "https://login.microsoftonline.com/{b2c tenant id (guid)}",
    "ClientId": "{client id of the UI app}",
    "ValidateAuthority": true
  },
  "ApiClient": {
    "Name": "B2CUserAdmin.API",
    "BaseAddress": "https://localhost:5001/api/",
    "Scopes": [
      "{full url of the api scope consumed by the UI app}"
    ]
  }
}