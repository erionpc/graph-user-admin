{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Hosting.Lifetime": "Debug"
    }
  },
  "TenantConfig": {
    "Authority": "https://login.microsoftonline.com/{Entra Id tenant id}",
    "ClientId": "{client id of the UI app}",
